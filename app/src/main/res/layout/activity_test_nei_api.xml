<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:clipToPadding="true"
    tools:context=".activity.TestNetApiActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:gravity="center_horizontal">

        <Button
            android:id="@+id/btn_prepareRegisterAcc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="预注册(获取验证码)"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_registerAcc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="注册"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_login"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="登录"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_prepare_bind_payment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="预绑卡"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_bind_payment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="绑卡"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_unbind_payment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="解绑"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_setTransPwd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="设置交易密码"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_estimateBuyFundFee"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="估算申购费用"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_buy_po"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="组合申购"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_user_bank_cards4C"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="获取用户已绑定的银行卡列表"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_user_po_list4C"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="获取我的基金列表"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_user_po_info4C"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="获取我的基金信息"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_fund_po_list4C"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="获取基金列表"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_fund_po_info4C"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="获取基金信息"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_single_redemption"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="单个赎回"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_group_redemption"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="组合赎回"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_article_details"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="文章详情"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_video_details"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="视频详情"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_call_camera"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="调用相机/相册功能"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_record_growth"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="点击图片记录成长"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_generate_album"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="成长相册"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_edu_scene"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="教育场景"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_wish_home_page"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="心愿首页"
            android:textColor="@android:color/black"/>

        <!-- 圈子 -->
        <Button
            android:id="@+id/btn_create_post"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="创建帖子"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_create_comment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="创建评论"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_create_thumb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="点赞"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_create_collection"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="收藏"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_create_report"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="举报评论"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_read_post"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="阅读帖子"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_Post_info"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="获取帖子详情"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_Post_list"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="帖子列表"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_Comment_list"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="帖子的评论列表"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_WishLists"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="获取心愿列表"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_CreateUserWish"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="创建用户心愿"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_GetWishListTypes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="获取心愿类型列表"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_DeleteUserWish"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="删除用户心愿"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_GetSurveyList"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="风险等级评测信息列表"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_EvaluateUserRiskLevel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="评估用户风险等级"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_PrepareChangeTransPwd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="修改交易密码前，对用户身份进行验证"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_ChangeTransPwd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="修改交易密码"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_ResetLoginPwd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="重置登录密码"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_GetLoginPwdVcode"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="重置登录密码验证码"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_Logoff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="用户退出"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_UpdateUserInfo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="更新用户信息"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_GetUserInfo4C"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="获取用户信息"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_GetUserCollectionList4C"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="获取用户收藏列表"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_GetUserMainPageInfo4C"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="获取我的主页信息"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_GetUserTradeHistory4C"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="获取用户组合交易历史记录"
            android:textColor="@android:color/black"/>

        <Button
            android:id="@+id/btn_CreateShareAlbum"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="创建分享相册"
            android:textColor="@android:color/black"/>

</LinearLayout>
</ScrollView>
